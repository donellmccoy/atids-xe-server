    "PoolSize": 16,
    "MaxRetryCount": 5,
    "MaxRetryDelay": "00:00:00:30",
    "EnablePooling": true,
    "UseInMemoryDatabase": false,
    "EnableDetailedErrors": true,
    "InMemoryDatabaseName": "AtidsXeDB",
    "QueryTrackingBehavior": "NoTracking",
    "ContextLifetime": "Transient",
    "OptionsLifetime": "Transient"
  },
  "QueryExecutionOptions": {
    "ExecutionTimeout": 240,
    "IncludeExceptionDetails": true,
    "MaxExecutionDepth": null,
    "MaxOperationComplexity": null,
    "QueryCacheSize": 100,
    "TracingPreference": "Never",
    "UseComplexityMultipliers": null,
    "ForceSerialExecution": false //Execution engine will be forced to execute serially since DbContext is not thread-safe
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "System": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
